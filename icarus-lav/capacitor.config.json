{
  "appId": "com.icarus.lav",
  "appName": "Icarus.LAV",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon",
      "iconColor": "#f472b6"
    },
    "Preferences": {
      "group": "IcarusLavGroup"
    },
    "Browser": {
      "presentationStyle": "fullscreen"
    }
  },
  "android": {
    "allowMixedContent": true
  },
  "server": {
    "cleartext": true,
    "androidScheme": "https",
    "allowNavigation": ["*"]
  }
}

{
  "appId": "com.icarus.app",
  "appName": "Icarus",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon",
      "iconColor": "#d4af37"
    },
    "Browser": {
      "presentationStyle": "fullscreen"
    }
  },
  "android": {
    "allowMixedContent": true
  },
  "server": {
    "cleartext": true,
    "androidScheme": "https",
    "allowNavigation": ["*"]
  }
}
